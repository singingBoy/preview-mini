.wrapper-title{
  padding: 27rpx 30rpx;
  font-size: 30rpx;
  color: #2A2A2A;
  line-height: 42rpx;
  background-color: #fff;
  font-weight: bold;
  position: relative;
}
.wrapper-title::before{
  content: '';
  position: absolute;
  bottom: 0;
  left: 30rpx;
  right: 30rpx;
  height: 2rpx;
  transform: scaleY(.5);
  background: #E7E7E7;
}

