  background-image: url("data:image/svg+xml;base64,");
}
.guide-icon-3{
  background-image: url("data:image/svg+xml;base64,");
}
.guide-icon-4{
  background-image: url("data:image/svg+xml;base64,");
}

.wrapper-title{
  padding: 27rpx 30rpx;
  font-size: 30rpx;
  color: #2A2A2A;
  line-height: 42rpx;
  background-color: #fff;
  font-weight: bold;
  position: relative;
}
.wrapper-title::before{
  content: '';
  position: absolute;
  bottom: 0;
  left: 30rpx;
  right: 30rpx;
  height: 2rpx;
  transform: scaleY(.5);
  background: #E7E7E7;
}
.wrapper-title-more{
  width: 100rpx;
  line-height: 42rpx;
  text-align: right;
  float: right;
  font-weight: normal;
  font-size: 26rpx;
  color: orangered;
}

/* 分类 */
.classify-wrapper{
  flex-wrap: wrap;
  padding: 40rpx 30rpx 20rpx;
  background-color: #fff;
}
.classify-item{
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 170rpx;
}
.classify-item-image{
  width: 80rpx;
  height: 80rpx;
}
.classify-item-name{
  font-size: 24rpx;
  color: #7f7f7f;
  margin: 15rpx 0 20rpx 0;
}

/* 广告 */
.ad-wrapper{
  padding: 0 30rpx;
  background-color: #fff;
}
.ad-image{
  width: 100%;
  height: 220rpx;
  border-radius: 8rpx;
}
.ad-title{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 500rpx;
  height: 200rpx;
  line-height: 200rpx;
  letter-spacing: 10rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
  background-color: rgba(0, 0, 0, .2);
  animation: scale-big .8s ease-in-out;
  animation-iteration-count: 3;
}
@keyframes scale-big{
  0%{
    font-size: 30rpx;
  }
  50%{
    font-size: 24rpx;
  }
  100%{
    font-size: 30rpx;
  }
}

