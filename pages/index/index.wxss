/* 导航 */
.search-bar{
  width: 100%;
  padding: 8rpx 16rpx;
  border-radius: 32rpx;
}
.keys-list {
  width: 100%;
  height: 90rpx;
  margin-left: 30rpx;
}

/* banner */
.banner-swipe{
  height: 500rpx;
}
.banner-img{
  width: 100%;
  height: 500rpx;
}
.banner-dots{
  position: absolute;
  bottom: 24rpx;
  right: 30rpx;
}
.dot-item {
  width: 8rpx;
  height: 8rpx;
  background: rgba(255,255,255,.5);
  border-radius: 4px;
  margin-right: 8rpx;
}
.dot-item:last-child {
  margin-right: 0;
}
.dot-active {
  width: 22rpx;
  background-color: #E41919;
}

/* 导航栏 */
.guide-wrapper{
  background-color: #ffffff;
  padding: 20rpx 30rpx;
  overflow: hidden;
}
.guide-item{
  width: 340rpx;
  height: 170rpx;
  background-size: 100% 100%;
  float: left;
  border-radius: 5rpx;
  position: relative;
}
.guide-item:nth-child(2n-1){
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}
.guide-item-1 {
  background-image: url("http://plsi1usd8.bkt.clouddn.com/guide-1.png");
}
.guide-item-2 {
  background-image: url("http://plsi1usd8.bkt.clouddn.com/guide-2.png");
}
.guide-item-3 {
  background-image: url("http://plsi1usd8.bkt.clouddn.com/guide-3.png");
}
.guide-item-4 {
  background: url("http://plsi1usd8.bkt.clouddn.com/guide-4.png");
}
.guide-title{
  padding: 10rpx 30rpx 5rpx;
  font-size: 30rpx;
  color: #2A2A2A;
}
.guide-description{
  padding: 0 30rpx 5rpx;
  font-size: 24rpx;
  color: #7f7f7f;
}
.guide-icon{
  position: absolute;
  top: 105rpx;
  right: -40rpx;
  height: 100%;
  width: 150rpx;
  background-size: 50%;
  background-repeat: no-repeat;
}
