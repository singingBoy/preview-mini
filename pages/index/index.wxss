/* banner */
swiper{
  height: 500rpx;
}
.banner-img{
  width: 100%;
  height: 500rpx;
}
.banner-dots{
  position: absolute;
  bottom: 24rpx;
  right: 30rpx;
}
.dot-item {
  width: 8rpx;
  height: 8rpx;
  background: rgba(255,255,255,.5);
  border-radius: 4px;
  margin-right: 8rpx;
}
.dot-item:last-child {
  margin-right: 0;
}
.dot-active {
  width: 22rpx;
  background-color: #E41919;
}

/* 导航栏 */
.guide-wrapper{
  background-color: #ffffff;
  padding: 20rpx 30rpx;
  overflow: hidden;
}
.guide-item{
  width: 340rpx;
  height: 170rpx;
  background-size: 100% 100%;
  float: left;
  border-radius: 5rpx;
  position: relative;
}
.guide-item:nth-child(2n-1){
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}
.guide-item-1 {
  background-image: url("http://plsi1usd8.bkt.clouddn.com/guide-1.png");
}
.guide-item-2 {
