.d-flex{
  display: flex;
}
.flex-column{
  flex-direction: column;
}
.position-relative{
  position: relative;
}
.position-absolute{
  position: absolute;
}
.align-items-center{
  align-items: center;
}
.justify-content-between{
  justify-content: space-between;
}
.justify-content-center{
  justify-content: center;
}

